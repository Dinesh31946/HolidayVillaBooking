{
  "rewrites": [
    {
      "source": "/booking/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/villa/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/about",
      "destination": "/index.html"
    },
    {
      "source": "/contact",
      "destination": "/index.html"
    },
    {
      "source": "/villas",
      "destination": "/index.html"
    },
    {
      "source": "/:path((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}