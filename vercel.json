{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "includeFiles": [
          "studio/dist/**", 
          "[FRONTEND_BUILD_OUTPUT_FOLDER]/**" 
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/studio/dist/$1"
    },
    {
      "source": "/admin",
      "destination": "/studio/dist/index.html"
    }
  ]
}